{
  "manifest_version": 2,
  "name": "Hotstar",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
  {
    "matches": [
        "http://www.hotstar.com/*",
        "http://getcdn.hotstar.com/*",
        "http://hlsplayer.net/*"
    ],
    "js": ["jquery-3.2.0.min.js","content.js"]
  }
],
  "browser_action": {
  "default_icon": "icon.png"
},
"permissions": [
  "activeTab","tabs", "http://*/*", "https://*/*"
]

}